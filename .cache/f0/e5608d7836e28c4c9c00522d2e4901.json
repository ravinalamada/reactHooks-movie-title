(e) {\n    const id = e.target.id;\n    console.log(id);\n    setMovies(movies.find(item => item.id === id));\n    setCount((prev) => prev.rt_score - 1);\n  }\n\n  function HandleClick (e) {\n    const id = e.target.id;\n    setMovies(movies.find(item => item.id === id));\n    setCount((prev) => prev.rt_score + 1);\n  }\n\n  return (\n    <section>\n      {movies && movies\n      .sort((a, b) => b.rt_score - a.rt_score)\n      .map(item => {\n        return (\n          <div key={item.id} className=\"container\">\n            <div className=\"wrapper\">\n              <h2>{item.title}</h2>\n              <p>{item.release_date}</p>\n              <p>{item.rt_score}</p>\n            </div>\n            <p>{item.description}</p>\n            <div className=\"wrapper\">\n              <p>{item.producer}</p>\n              <p>{item.director}</p>\n            </div>\n            <div>\n              <button className=\"like\" id={item.id} onClick={HandleIncrement}>Like</button>\n              <button className=\"unlike\" id={item.id} onClick={HandleDecrement}>Unlike</button>\n              <button className=\"delete\" id={item.id} onClick={HandleClick}>Delete</button>\n            </div>\n          </div>\n        )\n      })}\n    </section>\n  )\n\n}\n\n\n\n\nexport default MovieComponent;\n\n\n"},"lineCount":null}},"error":null,"hash":"12dbfa40d1fcd28fed8fc0a0bd807503","cacheData":{"env":{}}}